{
  "name": "pearpass-lib-vault-bare",
  "version": "0.0.29",
  "type": "module",
  "description": "A bare wrapper for vault management library",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint --fix .",
    "test": "jest",
    "bundle:android": "bare-pack -p android --linked --out bundles/autopass-android.bundle src/worklet/app.js",
    "bundle:ios": "bare-pack -p ios --linked --out bundles/autopass-ios.bundle src/worklet/app.js",
    "bundle-bare": "npm run bundle:android && npm run bundle:ios"
  },
  "dependencies": {
    "autopass": "=1.0.2",
    "bare-crypto": "=1.4.0",
    "bare-fs": "=4.0.1",
    "bare-pack": "=1.2.4",
    "bare-path": "=3.0.0",
    "bare-rpc": "=0.1.4",
    "corestore": "=6.18.4",
    "events": "=3.3.0",
    "expo-asset": "=11.0.4",
    "expo-file-system": "=18.0.10",
    "sodium-native": "=4.3.3",
    "udx-native": "=1.17.5"
  },
  "peerDependencies": {
    "bare-os": "=3.6.0",
    "react-native": "0.76.5",
    "react-native-bare-kit": "=0.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "tether-dev-docs": "github:noxtton/tether-dev-docs#v0.0.3"
  }
}
