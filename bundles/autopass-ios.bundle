      this._pool._lingering.add(this)
      this._timeout = setTimeout(this._lingeringClose.bind(this), LINGER_TIME)
      return
    }

    this._closed = true
    this.socket.close()
  }

  _unlinger () {
    if (this._timeout !== null) {
      clearTimeout(this._timeout)
      this._pool._lingering.delete(this)
      this._timeout = null
    }
  }

  get free () {
    return this._refs === 0
  }

  active () {
    this._refs++
    this._unlinger()
  }

  inactive () {
    this._refs--
    this._closeMaybe()
  }

  address () {
    return this.socket.address()
  }

  release () {
    if (this._released) return

    this._released = true
    this._reset()

    this._refs--
    this._closeMaybe()
  }
}

