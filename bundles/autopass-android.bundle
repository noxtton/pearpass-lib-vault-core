    if (i.expires) c.uint32.preencode(state, Math.floor(i.expires / 1000)) // store as secs
  },
  encode (state, i) {
    c.uint.encode(state, 1) // version
    c.uint.encode(state, (i.discoveryKey ? 1 : 0) | (i.expires ? 2 : 0) | (i.sensitive ? 4 : 0) | (i.testInvitation ? 8 : 0))
